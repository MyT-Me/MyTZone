<head>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.5/angular.min.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript" src="chartshelper.js"></script>
</head>
<div class="control-group">
    <span style="padding-left:73em"><a id="logout" href="#" class="btn btn-danger" ng-click = "Logout()">Logout</a></span>
</div>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<div id="chart_div"></div>

<body ng-controller="myController">
    <!-- <div id="high-container">Placeholder for chart</div> -->
</body>
<!--ending  G charts-->
<section ng-init="scoreRunner()">
    <div class="row">
    <nav1 class="col-md-12">
        <ol class="cd-multi-steps text-bottom count" id="status-buttons">
            <li class="visited"><a href="#0" ui-sref-active="active" ui-sref="base.educate">Education</a></li>
            <li class="visited"><a href="#0" ui-sref-active="active" ui-sref="base.work">Work</a></li>
            <li class="visited"><a href="#0" ui-sref-active="active" ui-sref="base.deeds">Deeds</a></li>
            <li class="visited"><a href="#0" ui-sref-active="active" ui-sref="base.skills">Skills</a></li>
            <li class="visited"><a href="#0" ui-sref-active="active" ui-sref="base.tools">Tools</a></li>
            <li class="current"><a href="#0" ui-sref-active="active" ui-sref="base.chart">T-Score</a></li>
        </ol>
    </nav1>
</div>
</section>
<!-- Deleted this from line below: ng-init= "scoreRunner()"" -->
<!-- What does the below line even do? -->
<div id="headingmain" ng-app="FormlyApp"></div>

<h1 id="heading_login">Charts</h1>
<div id="inside" class="container">
<div class="row">
    <div class="col-md-10">
    <div class="tscore" id="lower">
        <h3>My T-Top score</h3>
        <input class="score" type="number" name="score" value={{roundMe(fulltTop)}}>
        <br>
        <p>Delta: {{fulltTopDelta}}</p>
        <p>Previous Values: {{fulltTopPreString}}</p>
    </div>
    <div class="new_right">
        <div id='chartDiv' class="right"></div>
    </div>
    <div class="tscore" id="topper">
        <h3>My T-Stem score</h3>
        <input class="score" type="number" name="score" value={{roundMe(fulltStem)}}>
        <br>
        <p>Delta: {{fulltStemDelta}}</p>
        <p>Previous Values: {{fulltStemPreString}}</p>
    </div>
    <div class="new_right">
        <div id='chartDiv2' class="right"></div>
    </div>
    <div class="tscore" id="topper">
        <h3>My T-Score</h3>
        <input class="score" type="number" name="score" value={{roundMe(fulltScore)}}>
        <br>
        <p>Delta: {{fulltScoreDelta}}</p>
        <p>Previous Values: {{fulltScorePreString}}</p>
    </div>
    <div class="new_right">
        <div id='chartDiv2' class="right"></div>
    </div>

    <div id='myChart1'></div>
    <div id="w3" class="w3-light-grey">
        <div class="w3-container w3-center" style="height:20px;width:50px" id="bottom">
            
        </div>
    </div>
</div>
    <br>
    <div>
        <form id="tscoretop" name="form1" method="post" action="">
            <table>
                <thead><b>T-Top Scores</b></thead>
                <tr>
                    <td>Project Management</td>
                    <td>
                        <input type="text" name="num1" id="num1" value={{roundMe(managementScore)}}>
                    </td>
                    <td>Delta: {{managementScoreDelta}} Previous: {{managementScorePreString}}</td>
                </tr>
                <tr>
                    <td>Organizational Design</td>
                    <td>
                        <input type="text" name="num2" id="num2" value={{roundMe(designScore)}}>
                    </td>
                    <td>Delta: {{designScoreDelta}} Previous: {{designScorePreString}}</td>
                </tr>
                <tr>
                    <td>Communications</td>
                    <td>
                        <input type="text" name="num1" id="num3" value={{roundMe(communicationScore)}}>
                    </td>
                    <td>Delta: {{communicationScoreDelta}} Previous: {{communicationScorePreString}}</td>
                </tr>
                <tr>
                    <td>Critical Thinking</td>
                    <td>
                        <input type="text" name="num2" id="num4" value={{roundMe(criticalScore)}}>
                    </td>
                    <td>Delta: {{criticalScoreDelta}} Previous: {{criticalScorePreString}}</td>
                </tr>
                <tr>
                    <td>Teamwork</td>
                    <td>
                        <input type="text" name="num1" id="num5" value={{roundMe(teamworkScore)}}>
                    </td>
                    <td>Delta: {{teamworkScoreDelta}} Previous: {{teamworkScorePreString}}</td>
                </tr>
                <tr>
                    <td>Networking</td>
                    <td>
                        <input type="text" name="num2" id="num6" value={{roundMe(networkingScore)}}>
                    </td>
                    <td>Delta: {{networkingScoreDelta}} Previous: {{networkingScorePreString}}</td>
                </tr>
                <tr>
                    <td>Empathy</td>
                    <td>
                        <input type="text" name="num1" id="num7" value={{roundMe(empathyScore)}}>
                    </td>
                    <td>Delta: {{empathyScoreDelta}} Previous: {{empathyScorePreString}}</td>
                </tr>
                <tr>
                    <td>Perspective</td>
                    <td>
                        <input type="text" name="num2" id="num8" value={{roundMe(perspectiveScore)}}>
                    </td>
                    <td>Delta: {{perspectiveScoreDelta}} Previous: {{perspectiveScorePreString}}</td>
                </tr>
                <tr>
                    <td>Global Understanding</td>
                    <td>
                        <input type="text" name="num1" id="num9" value={{roundMe(globalUnderstandScore)}}>
                    </td>
                    <td>Delta: {{globalUnderstandScoreDelta}} Previous: {{globalUnderstandScorePreString}}</td>
                </tr>
                <tr>
                    <td><b>Total T-Top Score</b></td>
                    <td>
                        <input type="text" name="sum" id="sum" readonly value={{roundMe(fulltTop)}}>
                    </td>
                    <td>Delta: {{fulltTopDelta}} Previous: {{fulltTopPreString}}</td>
                </tr>
            </table>
        </form>
    </div>
    <br>
    <div id="right">
        <form id="tscorebot" name="form2" method="post" action="">
            <table>
                <thead><b>T-Stem Scores</b></thead>
                <tr>
                    <td>Memberships, authorships and recognition</td>
                    <td>
                        <input type="text" name="num10" id="num10" value={{roundMe(membershipScore)}}>
                    </td>
                    <td>Delta: {{membershipScoreDelta}} Previous: {{membershipScorePreString}}</td>
                </tr>
                <tr>
